{
	"name": "localize-lens",
	"displayName": "Localize Lens",
	"description": "Quickly identify if a function only has local references.",
	"author": {
		"name": "Matthew Matthiesen"
	},
	"license": "MIT",
	"publisher": "mmatthiesen",
	"version": "1.0.0",
	"icon": "assets/icon.png",
	"keywords": [
		"refactor",
		"global",
		"local",
		"reference",
		"occurence",
		"al",
		"nav",
		"bc365",
		"business central"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/mjmatthiesen/localize-lens"
	},
	"bugs": {
		"url": "https://github.com/mjmatthiesen/localize-lens/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Formatters",
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onLanguage:al"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
